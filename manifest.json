{
    "manifest_version": 3,
    "name": "Mi Extensión",
    "version": "1.0",
    "description": "Una extensión de Chrome con TypeScript",
    "permissions": ["storage", "activeTab"],
    "background": {
      "service_worker": "dist/background.js"
    },
    "action": {
      "default_popup": "popup/popup.html"
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["dist/content.js"]
      }
    ]
  }
  